{
  "manifest_version": 3,
  "name": "Leetcode Community Extension",
  "version": "1.0",
  "description": "A basic Chrome extension",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/CSSLeetCodeCommunity.png",
      "48": "images/CSSLeetCodeCommunity.png",
      "128": "images/CSSLeetCodeCommunity.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}